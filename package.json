{
  "name": "vesium-lib",
  "type": "module",
  "version": "1.0.1-beta.63",
  "private": true,
  "description": "Vue component and composition-api library for Cesium.",
  "license": "MIT",
  "homepage": "https://github.com/vesiumjs/vesium",
  "repository": {
    "url": "https://github.com/vesiumjs/vesium"
  },
  "bugs": {
    "url": "https://github.com/vesiumjs/vesium/issues"
  },
  "keywords": [
    "vue",
    "cesium",
    "vueuse",
    "hooks",
    "composition-api"
  ],
  "engines": {
    "node": ">=22"
  },
  "scripts": {
    "docs:dev": "vitepress dev packages --host",
    "docs:build": "npm run build:types && vitepress build packages",
    "docs:preview": "vitepress preview packages",
    "build:types": "vue-tsc -p ./tsconfig.build.json --emitDeclarationOnly",
    "test:unit": "vitest",
    "test:e2e": "start-server-and-test preview http://localhost:4173 'cypress run --e2e'",
    "test:e2e:dev": "start-server-and-test 'vite dev --port 4173' http://localhost:4173 'cypress open --e2e'",
    "type-check": "vue-tsc --build --force",
    "lint": "eslint . --fix",
    "taze": "taze -w -r major minor patch",
    "release": "bumpp -r",
    "==========[CI/CD]===========": "",
    "build:ci": "tsdown",
    "publish:ci": "tsx ./scripts/publish"
  },
  "dependencies": {
    "@unocss/reset": "catalog:",
    "@vesium/plot": "workspace:*",
    "@vueuse/core": "catalog:",
    "cesium": "catalog:",
    "pinia": "catalog:",
    "vesium": "workspace:*",
    "vue": "catalog:",
    "vue-router": "catalog:"
  },
